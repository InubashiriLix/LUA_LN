{
  "Lua": {
    "runtime": {
      "version": "LuaJIT",
      "path": [
        "?.lua",
        "?/init.lua"
      ]
    },
    "workspace": {
      "checkThirdParty": true,
      "library": [
        "./test",
        "./src",
        "./lib",
        "${workspace}/**",
        "/usr/local/lib/luarocks/rocks-5.4/",
        "${3rd}/lfs/library",
        "${3rd}/cjson/library"
      ]
    },
    "diagnostics": {
      "globals": [
        "vim",
        "lfs",
        "cjson"
      ]
    }
  }
}
